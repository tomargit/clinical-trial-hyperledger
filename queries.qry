query selectVisitByPatient {
  description: "Select all commodities"
  statement:
      SELECT com.incedoinc.clinical.Visit
        WHERE (patient==_$userId)
}


query selectVisitByInvestigator {
  description: "Select all commodities"
  statement:
      SELECT com.incedoinc.clinical.Visit
        WHERE (investigator==_$userId)
}

